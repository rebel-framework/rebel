{
  "name": "@rebel-framework/database",
  "version": "0.2.4",
  "private": false,
  "description": "Rebel Framework database utility to work with DynamoDB",
  "author": "Ben Villiere",
  "homepage": "https://github.com/rebel-framework/rebel#readme",
  "license": "MIT",
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebel-framework/rebel.git"
  },
  "scripts": {
    "build": "tsc && babel src --root-mode upward --out-dir build --extensions '.ts'",
    "watch": "tsc && babel src --root-mode upward --out-dir build --extensions '.ts' --watch",
    "type-check": "npx tsc",
    "format": "npx prettier .",
    "lint": "npx eslint src/**.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.378.0"
  },
  "bugs": {
    "url": "https://github.com/rebel-framework/rebel/issues"
  },
  "keywords": [
    "serverless",
    "typescript",
    "aws",
    "framework",
    "full-stack"
  ]
}
